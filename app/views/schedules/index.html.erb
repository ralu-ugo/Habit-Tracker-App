
<%= month_calendar do |date| %>
  <%= date %>
<% end %>

<%= week_calendar(number_of_weeks: 2) do |date| %>
  <%= date %>
<% end %>

<%= calendar(number_of_days: 4) do |date| %>
  <%= date %>
<% end %>

<%= calendar(number_of_days: 1) do |date| %>
  <%= date %>
<% end %>


<%= calendar(start_date_param: Date.now) do |date| %>
  <%= date %>
<% end %>
